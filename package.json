    "mockjs": "^1.1.0",
    "moment": "^2.26.0",
    "postcss-px2rem": "^0.3.0",
    "qs": "^6.9.4",
    "vant": "^2.8.4",
    "vue": "^2.6.11",
    "vue-router": "^3.3.2",
    "vuetify": "^2.2.31",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-eslint": "^10.1.0",
